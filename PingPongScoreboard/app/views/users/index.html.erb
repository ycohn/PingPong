<div class="container">
  <table class="table table-striped table-hover ">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>High Score</th>
      </tr>
    </thead>
    <tbody>
    <%@users.each_with_index do |user, index|%>
      <tr class="info">
        <td id="name-<%=index %>"><%=user.name%></td>
        <td id="email-<%=index %>"><%=user.email%></td>
        <td id="high_score-<%=index %>"><%=user.high_score%></td>
      </tr>
      <% end %>
    </tbody>
  </table> 
</div>
